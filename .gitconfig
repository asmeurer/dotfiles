[user]
	name = Aaron Meurer
	email = asmeurer@gmail.com

[color]
	diff  = auto
	status= auto
	branch= auto
	interactive = true

[color "diff"]
	new = white
[alias]
	ci = commit --interactive
	di = diff --color-words
	st = status
	co = checkout
	pu = push github
	logl = log --pretty=oneline --abbrev-commit
	gr = grep -i --color
	backup = !git-backup.sh
	fe = fetch --all
	weekreport = shortlog --since=1.weeks --author=asmeurer
	br = branch
	detach = !git checkout $(git rev-parse HEAD)

[sendemail]
	o = sympy-patches@googlegroups.com
	envelopesender = asmeurer@gmail.com
	smtpserver = smtp.gmail.com
	smtpuser = asmeurer@gmail.com
	smtpssl = true
	aliasfiletype = mailrc
[i18n]
	commitEncoding = utf-8
[push]
	default = matching
[github]
	user = asmeurer
	token = 2927afd5e06ef4c7401be97e3b3a8dee
[rerere]
	enabled = true
